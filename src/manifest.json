{
    "manifest_version": 2,
    "name": "Hyperocto",
    "version": "0.3.3",

    "default_locale": "en",
    "description": "__MSG_MANIFEST_DESCRIPTION__",
    "icons": {
        "16": "icons/16.png",
        "19": "icons/19.png",
        "32": "icons/32.png",
        "38": "icons/38.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
        "96": "icons/96.png",
        "128": "icons/128.png"
    },

    "author": "Megaman",
    "background": {
        "scripts": [
            "message-id.js",
            "background-script.js"
        ]
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "all_frames": true,
            "js": [
                "message-id.js",
                "content-script.js"
            ]
        }
    ],

    "permissions": [
        "contextMenus",
        "tabs"
    ]
}
